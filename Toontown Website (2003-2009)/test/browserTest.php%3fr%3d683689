        headerPromoRight_Reg = new Image();
        headerPromoRight_Reg.src = "http://play.toontown.com/shared/images/topnav/hdrPromoRight_reg.gif";

        headerPromoRight_Over = new Image();
        headerPromoRight_Over.src = "http://play.toontown.com/shared/images/topnav/hdrPromoRight_over.gif";

        function img_over(imgName) {
          imgOn = eval(imgName + "_Over.src");
          document [imgName].src = imgOn;
        }

        function img_reg(imgName) {
          imgOn = eval(imgName + "_Reg.src");
          document [imgName].src = imgOn;
        }

